{
  "name": "bstats/pocketmine-mp",
  "type": "library",
  "description": "bStats Metrics for PocketMine-MP plugins",
  "keywords": [
    "bstats",
    "pocketmine-mp"
  ],
  "homepage": "https://github.com/Bastian/bstats-pmmp",
  "license": "MIT",
  "authors": [
    {
      "name": "Bastian",
      "homepage": "https://bStats.org",
      "role": "Developer"
    }
  ],
  "require": {
    "pocketmine/pocketmine-mp": "^5.0.0",
    "php": ">=8.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "bStats\\PocketmineMp\\": "src/"
    }
  },
  "extra": {
    "virion": {
      "spec": "3.1",
      "namespace-root": "bStats\\PocketmineMp"
    }
  }
}
